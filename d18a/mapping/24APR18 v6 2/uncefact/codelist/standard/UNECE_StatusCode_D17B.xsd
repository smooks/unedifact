<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:clm64405="urn:un:unece:uncefact:codelist:standard:UNECE:StatusCode:D17B" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" targetNamespace="urn:un:unece:uncefact:codelist:standard:UNECE:StatusCode:D17B" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.3">
