  <xsd:element name="StatusCode" type="clm64405:StatusCodeContentType"/>
<!-- ================================================================== -->
<!-- ===== Type Definitions                                       ===== -->
<!-- ================================================================== -->
<!-- ===== Type Definition: StatusCodeContentType                 ===== -->
<!-- ================================================================== -->
  <xsd:simpleType name="StatusCodeContentType">
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="1">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>To be done</ccts:Name>
            <ccts:Description>Remark that the requested service in the order remains outstanding.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="1">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Arrival, completed</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignment/equipment/means of transport has arrived.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="2">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Done</ccts:Name>
            <ccts:Description>The instruction has been completed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="2">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Loading, authorized</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
Authorisation to load has been given.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="3">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Passed on</ccts:Name>
            <ccts:Description>The information has been passed on.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="3">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Arrival, in defective condition</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignment/equipment or a means of transport has arrived in a defective condition.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="4">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Final</ccts:Name>
            <ccts:Description>The amount has the status of finality.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="4">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Defective equipment returned to service</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The equipment previously the subject of a 'defective condition' status has been returned to service.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="5">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Subject to final payment</ccts:Name>
            <ccts:Description>The amount is subject to finality.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="5">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Process, begun</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The process has begun.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="6">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Minimum</ccts:Name>
            <ccts:Description>The amount quoted is a minimum tariff.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="6">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Booking, completed</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignment/equipment or means of transport has been booked.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="7">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Fixed</ccts:Name>
            <ccts:Description>The amount quoted is a fixed tariff.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="7">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Booking, cancelled</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The booking of goods/consignment/equipment or means of transport has been cancelled.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="8">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Maximum</ccts:Name>
            <ccts:Description>The amount quoted is a maximum tariff.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="8">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cleared, import restrictions</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignment/equipment/means of transport held for import restriction purposes, has been released for import.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="9">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Information</ccts:Name>
            <ccts:Description>The amount is quoted for information only, it is not part of the charges to be deducted or added.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="9">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cleared, export restrictions</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignment/equipment/means of transport held for export restriction purposes, has been released for export.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="10">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>0 day available</ccts:Name>
            <ccts:Description>The amount is available today.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="10">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cleared, by agriculture, food or fisheries authorities</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignment/equipment/means of transport has been cleared by agriculture, food or fisheries authorities.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="11">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>1 day available</ccts:Name>
            <ccts:Description>The amount is available on the next banking office day after the booking date.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="11">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cleared, by port authority</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignment/equipment/means of transport has been cleared by port authority.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="12">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>2 days available</ccts:Name>
            <ccts:Description>The amount is available on the second banking office day after the booking date.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="12">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cleared, by customs</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignment/equipment/means of transport has been cleared by customs.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="13">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>3 days available</ccts:Name>
            <ccts:Description>The amount available on the third banking office day after the booking date.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="13">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Collection/pick-up, completed</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignment/equipment has been collected/picked-up.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="14">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Uncollected funds</ccts:Name>
            <ccts:Description>Funds not collected by beneficiary.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="14">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Process, completed</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The process has been completed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="15">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Nil</ccts:Name>
            <ccts:Description>The amount or quantity is zero.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="15">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Consolidated</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignments have been consolidated.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="16">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>None advised</ccts:Name>
            <ccts:Description>No status or condition has been advised.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="16">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Crossed border</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignments/equipment/means of transport has crossed a border.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="17">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Requested</ccts:Name>
            <ccts:Description>A status or condition has been requested.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="17">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Customs clearance, refused</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignment/equipment/means of transport has been refused to be cleared by the customs authorities.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="18">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Free of charge</ccts:Name>
            <ccts:Description>No charge is to be made.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="18">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Damaged in the course of transportation</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignment/equipment has been damaged in the course of transportation.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="19">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Rounded</ccts:Name>
            <ccts:Description>The quantity has been the subject of a rounding operation.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="19">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Equipment, damage quoted for</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
Damaged equipment has been assessed and a repair quotation has been sent.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="20">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Permanent</ccts:Name>
            <ccts:Description>Intended to last or function indefinitely.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="20">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Delayed, in the course of transportation</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignment/equipment/means of transport has been delayed in the course of transportation.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="21">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Temporary</ccts:Name>
            <ccts:Description>Lasting or intended to last or function for a limited time.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="21">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Delivery, completed</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignment/equipment has been delivered.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="22">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Subject to agreed condition</ccts:Name>
            <ccts:Description>Subject to agreed condition.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="22">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Delivery, completed as per instruction</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
Delivery of the goods/consignment/equipment has been completed as per instruction.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="23">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Added</ccts:Name>
            <ccts:Description>In addition to.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="23">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Delivery, not completed</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
Delivery of the goods/consignment/equipment has not been completed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="24">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Deducted</ccts:Name>
            <ccts:Description>A deduction from.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="24">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Departure, completed</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The means of transport has departed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="25">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Included</ccts:Name>
            <ccts:Description>Is included in.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="25">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Departure, delayed</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The means of transport has been delayed in departure.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="26">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Subject to clearing</ccts:Name>
            <ccts:Description>Clearing operations are to be applied.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="26">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Deramped</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The equipment has been removed from a position above another type of equipment and/or on a means of transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="27">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Subject to bilaterally agreed condition</ccts:Name>
            <ccts:Description>Bilaterally agreed conditions are to be applied.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="27">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Despatch, completed</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignment/equipment has been despatched.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="28">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Transaction expected to be executed</ccts:Name>
            <ccts:Description>The transaction is expected to be executed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="28">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Stripped</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignment/equipment has been unloaded from a piece of equipment in which they were transported.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="29">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Debit</ccts:Name>
            <ccts:Description>The object has a status of debit.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="29">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Unloaded</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignment/equipment has been unloaded from a means of transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="30">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Credit</ccts:Name>
            <ccts:Description>The object has a status of credit.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="30">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Empty on inspection</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The package/equipment has been found to be empty on inspection.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="31">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Positive debit</ccts:Name>
            <ccts:Description>The object has a status of positive debit.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="31">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>En route</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The goods/consignment/equipment/means of transport is moving to destination.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="32">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Negative debit</ccts:Name>
            <ccts:Description>The object has a status of negative debit.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="32">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Equipment, returned from repair</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The equipment has been returned from repair.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="33">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Positive credit</ccts:Name>
            <ccts:Description>The object has a status of positive credit.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="33">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Equipment, sent for repair</ccts:Name>
            <ccts:Description>[UN/Rec24 transport status]
The equipment has been sent for repair.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="34">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Negative credit</ccts:Name>
            <ccts:Description>The object has a status of negative credit.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="34">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
