<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:clm64053="urn:un:unece:uncefact:codelist:standard:UNECE:DeliveryTermsCode:2010" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" targetNamespace="urn:un:unece:uncefact:codelist:standard:UNECE:DeliveryTermsCode:2010" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
<!-- ======================================================================= -->
<!-- ===== Element Declarations                                        ===== -->
<!-- ======================================================================= -->
<!-- ===== Root Element                                                ===== -->
<!-- ======================================================================= -->
  <xsd:element name="DeliveryTermsCode" type="clm64053:DeliveryTermsCodeContentType"/>
<!-- ================================================================== -->
<!-- ===== Type Definitions                                       ===== -->
<!-- ================================================================== -->
<!-- ===== Type Definition: DeliveryTermsCodeContentType          ===== -->
<!-- ================================================================== -->
  <xsd:simpleType name="DeliveryTermsCodeContentType">
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="1">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Delivery arranged by the supplier</ccts:Name>
            <ccts:Description>Indicates that the supplier will arrange delivery of the goods.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="2">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Delivery arranged by logistic service provider</ccts:Name>
            <ccts:Description>Code indicating that the logistic service provider has arranged the delivery of goods.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CFR">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cost and Freight (insert named port of destination)</ccts:Name>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CIF">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cost, Insurance and Freight (insert named port of destination)</ccts:Name>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CIP">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Carriage and Insurance Paid to (insert named place of destination)</ccts:Name>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CPT">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Carriage Paid To (insert named place of destination)</ccts:Name>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DAP">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Delivered At Place (insert named place of destination)</ccts:Name>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DAT">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Delivered At Terminal (insert named terminal at port or place of destination)</ccts:Name>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DDP">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Delivered Duty Paid (insert named place of destination)</ccts:Name>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="EXW">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Ex Works (insert named place of delivery)</ccts:Name>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="FAS">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Free Alongside Ship (insert named port of shipment)</ccts:Name>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="FCA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Free Carrier (insert named place of delivery)</ccts:Name>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="FOB">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Free On Board (insert named port of shipment)</ccts:Name>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>
