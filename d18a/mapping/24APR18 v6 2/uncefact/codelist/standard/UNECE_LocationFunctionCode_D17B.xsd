            <ccts:Name>Country of destination of equipment</ccts:Name>
            <ccts:Description>Country of the last place where the equipment will stop.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="214">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Aircraft airport stand</ccts:Name>
            <ccts:Description>Code used to specify the airport stand allocated to the aircraft.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="215">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Airport passenger terminal</ccts:Name>
            <ccts:Description>Code used to specify the airport terminal used for the embarking or disembarking of passengers.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="216">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Previous berth</ccts:Name>
            <ccts:Description>Place or location in a port where a vessel was previously moored.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="217">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Next berth</ccts:Name>
            <ccts:Description>Place or location in a port where a vessel will be moored, after moving from the current location.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="218">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Entity location</ccts:Name>
            <ccts:Description>Identifies the place or location of the entity.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="219">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Goods depot</ccts:Name>
            <ccts:Description>Depot where goods are received and are available for pick-up.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="220">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Disinfecting place</ccts:Name>
            <ccts:Description>Place where disinfection has been or must be performed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="221">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Harbour rail station</ccts:Name>
            <ccts:Description>Rail station servicing a harbour.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="222">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of live animal care</ccts:Name>
            <ccts:Description>Place where live animal care has been or must be provided.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="223">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Phytosanitary control place</ccts:Name>
            <ccts:Description>Place where phytosanitary control has been or must be performed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="224">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place for re-icing or de-icing</ccts:Name>
            <ccts:Description>Place where re-icing or de-icing has been or must be performed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="225">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of refuelling</ccts:Name>
            <ccts:Description>Place where refuelling has been or must be performed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="226">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of provision of an unexpected service</ccts:Name>
            <ccts:Description>Place where an unexpected service has been or must be provided.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="227">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Private container terminal</ccts:Name>
            <ccts:Description>Container terminal managed or owned by a private company.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="228">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Railway container terminal</ccts:Name>
            <ccts:Description>Container terminal managed or owned by a railway company.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="229">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Inspection site</ccts:Name>
            <ccts:Description>The site where an inspection takes or took place.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="230">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Request only stop</ccts:Name>
            <ccts:Description>A location where a stop is only made on request.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="231">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Grid area</ccts:Name>
            <ccts:Description>A section of a grid.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="232">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Source power area</ccts:Name>
            <ccts:Description>The area that is the source of power.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="233">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Sink power area</ccts:Name>
            <ccts:Description>The area that is the destination of power.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="234">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Scheduled berth</ccts:Name>
            <ccts:Description>Place or location in a port where a vessel is scheduled to be moored.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="235">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Scheduled berth, bow</ccts:Name>
            <ccts:Description>Place or location in a port where the bow of a vessel is scheduled to berth.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="236">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Scheduled berth, stern</ccts:Name>
            <ccts:Description>Place or location in a port where the stern of a vessel is scheduled to berth.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="237">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Balance settlement area</ccts:Name>
            <ccts:Description>An area where common rules for balance settlement applies.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="238">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Market area</ccts:Name>
            <ccts:Description>An  area with common trading rules.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="239">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Metering grid area</ccts:Name>
            <ccts:Description>A physical area where consumption, production and exchange can be metered.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="240">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Climate zone</ccts:Name>
            <ccts:Description>A geographical area where the climate has common characteristics.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="241">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of birth</ccts:Name>
            <ccts:Description>Country where a person or an animal was born.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="242">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of fattening</ccts:Name>
            <ccts:Description>Country where an animal has been fattened.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="243">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of slaughter</ccts:Name>
            <ccts:Description>Country where an animal has been slaughtered.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="244">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of meat cutting</ccts:Name>
            <ccts:Description>Country where the meat is cut into pieces.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="245">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Meat cutting location</ccts:Name>
            <ccts:Description>A location where the meat is cut into pieces.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="246">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Slaughterhouse</ccts:Name>
            <ccts:Description>Place for the slaughter of animals as food.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="247">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of meat mincing</ccts:Name>
            <ccts:Description>Country where meat has been cut into very small pieces using a meat grinder.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="248">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of discharge and loading</ccts:Name>
            <ccts:Description>Place at which a means of transport is performing both discharge and loading operations, e.g. seaport, airport, freight terminal, rail station.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="249">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cargo facility location</ccts:Name>
            <ccts:Description>Name of the terminal, warehouse or yard where the goods are to be on or offloaded.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="250">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Tourist point of interest</ccts:Name>
            <ccts:Description>Code to specify that the location is a tourist point of interest.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="251">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Customs office of payment</ccts:Name>
            <ccts:Description>Place where Customs duties/taxes/fees have to be paid.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="252">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Conveyance facility location at departure</ccts:Name>
            <ccts:Description>Name of the location of the last facility (e.g. terminal, warehouse or yard) from which the conveyance will depart.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="253">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Conveyance facility location at arrival</ccts:Name>
            <ccts:Description>Name of the location of the initial facility (e.g. terminal, warehouse or yard) where the conveyance will arrive.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="254">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Bus station</ccts:Name>
            <ccts:Description>Name or identification of a bus station.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="255">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Ferry terminal</ccts:Name>
            <ccts:Description>Name or identification of a ferry terminal.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="256">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of packing</ccts:Name>
            <ccts:Description>Place where goods are packaged.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="257">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of assembly</ccts:Name>
            <ccts:Description>Country where product is assembled.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="258">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Town sales office</ccts:Name>
            <ccts:Description>The location is a town sales office.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="259">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Travel agency</ccts:Name>
            <ccts:Description>The location is a travel agency.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="260">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Inland clearance depot</ccts:Name>
            <ccts:Description>Depot where goods are cleared by the customs authorities or other governmental authorities in the interior of a country.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="261">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of final production</ccts:Name>
            <ccts:Description>Place where the production of the item was finalised.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="262">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of growth</ccts:Name>
            <ccts:Description>Place where the product was grown.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="263">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of intermediate production</ccts:Name>
            <ccts:Description>Place of any processing prior to final production.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="264">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of nutrient origin</ccts:Name>
            <ccts:Description>Place where the nutrient, or item of nourishing food, originated.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="265">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of package material production</ccts:Name>
            <ccts:Description>Place where the material used for packaging was produced.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="266">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of processing</ccts:Name>
            <ccts:Description>The place where the commodity was processed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="267">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of species origin</ccts:Name>
            <ccts:Description>The place where the species was taken from the wild, or the place where the species was born, artificially propagated, grown or harvested.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="268">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of catch</ccts:Name>
            <ccts:Description>Place where the animal was caught, e.g. area of the ocean where the fish was harvested.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="269">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Government appeal office</ccts:Name>
            <ccts:Description>The location of a government office at which an appeal can be lodged.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="270">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Regulatory office of cross-border goods entry</ccts:Name>
            <ccts:Description>To identify the regulatory office at which the goods enter or are intended to enter the customs territory of destination.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="271">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Regulatory office of cross-border goods exit</ccts:Name>
            <ccts:Description>To identify the regulatory office at which the goods leave or are intended to leave the customs territory of dispatch.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="272">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Government approved establishment</ccts:Name>
            <ccts:Description>Facility approved for a specific purpose by a government authority.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="273">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Free trade zone</ccts:Name>
            <ccts:Description>A special area of a country where some normal trade barriers (e.g. tariffs and quotas) are eliminated, a.k.a. foreign free zone.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="274">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of physical examination</ccts:Name>
            <ccts:Description>A place where goods are to be examined.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="275">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Permitted location</ccts:Name>
            <ccts:Description>The location at which goods identified on the license, permit, certificate or other document are allowed to be moved or otherwise handled.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="276">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Landing location</ccts:Name>
            <ccts:Description>The place where a means of transport may be parked or tied up, e.g. wharf, quay, railyard, parking lot.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="277">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of loading on final means of transport</ccts:Name>
            <ccts:Description>Place where a consignment is placed aboard the means of transport which enters the destination customs territory.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="278">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of regulatory declaration review</ccts:Name>
            <ccts:Description>Location where the declaration is reviewed by appropriate government agency.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="279">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Travel document issuing country or political entity</ccts:Name>
            <ccts:Description>The country or political entity issuing the travel document.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="280">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Compliant facility, registered</ccts:Name>
            <ccts:Description>A facility that has been formally documented as compliant with regulations or statutes by a regulatory authority.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="281">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Bonded warehouse</ccts:Name>
            <ccts:Description>A building or other secured area in which dutiable goods may be stored, manipulated, or undergo manufacturing operations without payment of duty.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="282">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Goods destruction site, designated</ccts:Name>
            <ccts:Description>A place designated for the destruction of goods.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="283">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Goods disposal location, designated</ccts:Name>
            <ccts:Description>A place designated for the disposal of goods.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="284">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Incineration facility</ccts:Name>
            <ccts:Description>Facility with a furnace or apparatus for consuming materials completely or reducing materials to ash by burning.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="285">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Laboratory</ccts:Name>
            <ccts:Description>A facility equipped for scientific experimentation, research, testing or determination.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="286">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of assembly</ccts:Name>
            <ccts:Description>Facility where product is assembled.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="287">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Processing site</ccts:Name>
            <ccts:Description>Place where an operation or a series of operations are performed in the making or treatment of a product.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
