<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:clm63227="urn:un:unece:uncefact:codelist:standard:UNECE:LocationFunctionCode:D17B" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" targetNamespace="urn:un:unece:uncefact:codelist:standard:UNECE:LocationFunctionCode:D17B" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.4">
<!-- ======================================================================= -->
<!-- ===== Element Declarations                                        ===== -->
<!-- ======================================================================= -->
<!-- ===== Root Element                                                ===== -->
<!-- ======================================================================= -->
  <xsd:element name="LocationFunctionCode" type="clm63227:LocationFunctionCodeContentType"/>
<!-- ================================================================== -->
<!-- ===== Type Definitions                                       ===== -->
<!-- ================================================================== -->
<!-- ===== Type Definition: LocationFunctionCodeContentType       ===== -->
<!-- ================================================================== -->
  <xsd:simpleType name="LocationFunctionCodeContentType">
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="1">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of terms of delivery</ccts:Name>
            <ccts:Description>(3018) Point or port of departure, shipment or destination, as required under the applicable terms of delivery, e.g. Incoterms.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="2">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Payment location</ccts:Name>
            <ccts:Description>[3109] To identify the place where the payment has been or should be made.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="3">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Tare check place</ccts:Name>
            <ccts:Description>Place where equipment tare has been or must be checked for official assessment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="4">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Goods receipt place</ccts:Name>
            <ccts:Description>[3160] Place at which the goods have been received.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="5">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of departure</ccts:Name>
            <ccts:Description>(3214) Port, airport or other location from which a means of transport or transport equipment is scheduled to depart or has departed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="6">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Ward bed</ccts:Name>
            <ccts:Description>A bed in a ward.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="7">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of delivery</ccts:Name>
            <ccts:Description>(3246) Place to which the goods are to be finally delivered under transport contract terms (operational term).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="8">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of destination</ccts:Name>
            <ccts:Description>(3258) Port, airport or other location to which a means of transport or transport equipment is destined.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="9">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of loading</ccts:Name>
            <ccts:Description>[3334] Seaport, airport, freight terminal, rail station or other place at which the goods (cargo) are loaded on to the means of transport being used for their carriage.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="10">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of acceptance</ccts:Name>
            <ccts:Description>[3348] Place where the goods are taken over by the carrier.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="11">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of discharge</ccts:Name>
            <ccts:Description>[3392] Seaport, airport, freight terminal, rail station or other place at which goods are unloaded from the means of transport having been used for their carriage.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="12">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Port of discharge</ccts:Name>
            <ccts:Description>Port at which the goods are discharged from the vessel used for their transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="13">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of transhipment</ccts:Name>
            <ccts:Description>[3424] Place where goods are to be or have been transferred from one means of transport to another during the course of one transport operation.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="14">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Goods item storage location</ccts:Name>
            <ccts:Description>[3385] To identify the place where a goods item is located.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="15">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of transfer responsibility</ccts:Name>
            <ccts:Description>Place where the responsibility is transferred.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="16">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of transfer of ownership</ccts:Name>
            <ccts:Description>Place where the ownership of the goods is transferred.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="17">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Border crossing place</ccts:Name>
            <ccts:Description>Place where goods are transported across a country border.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="18">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Warehouse</ccts:Name>
            <ccts:Description>[3156] Warehouse where a particular consignment is to be or has been stored.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="19">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Factory/plant</ccts:Name>
            <ccts:Description>Factory/plant relevant for a particular consignment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="20">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of ultimate destination of goods</ccts:Name>
            <ccts:Description>Place where goods will ultimately be delivered.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="21">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Terms of sale place</ccts:Name>
            <ccts:Description>Place of departure, shipment or destination as specified in the terms of sale agreed between the parties.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="22">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Customs clearance location</ccts:Name>
            <ccts:Description>[3080] Place at which Customs clearance should or has taken place.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="23">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Port of release</ccts:Name>
            <ccts:Description>Port where goods are released from Customs custody.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="24">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Port of entry</ccts:Name>
            <ccts:Description>Port where final documentation is filed for Customs Entry processing.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="25">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country</ccts:Name>
            <ccts:Description>Country relevant for a particular transaction.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="26">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>City</ccts:Name>
            <ccts:Description>City or town relevant for a particular transaction or consignment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="27">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of origin</ccts:Name>
            <ccts:Description>[3239] To identify the country in which the goods have been produced or manufactured, according to criteria laid down for the application of the Customs tariff or quantitative restrictions, or any measure related to trade.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="28">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of destination of goods</ccts:Name>
            <ccts:Description>Country to which the goods are to be delivered.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="29">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Railway station</ccts:Name>
            <ccts:Description>Name or identification of a railway station/yard relevant to a particular consignment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="30">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of source</ccts:Name>
            <ccts:Description>Country in which raw material or components were originally produced prior to manufacture or assembly in another country.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="31">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Building</ccts:Name>
            <ccts:Description>A building or part thereof relevant to a particular consignment or transaction.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="32">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Beginning of chargeable section</ccts:Name>
            <ccts:Description>First rail station in a predefined section of the chargeable voyage. A complete voyage may be divided in sections, even within one country, that are separately chargeable using different tariff rules (split tariffs).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="33">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Baseport of discharge</ccts:Name>
            <ccts:Description>[3356] Place at which the cargo is discharged or unloaded from a means of transport according to the transport contract. The goods may or may not be discharged from the main means of transport at this place or port.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="34">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Baseport of loading</ccts:Name>
            <ccts:Description>[3322] Place at which the cargo is loaded on a means of transport according to the transport contract. The goods may or may not be loaded on the main means of transport at this place or port.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="35">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Exportation country</ccts:Name>
            <ccts:Description>[3220] Country from which the goods were initially exported to the importing country without any commercial transaction taking place in intermediate countries. Synonym: Country whence consigned. Country of despatch: Country from which goods are despatched between countries of a Customs union.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="36">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of ultimate destination</ccts:Name>
            <ccts:Description>(3216) Country known to the consignor or his agent at the time of despatch to be the final country to which the goods are to be delivered.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="37">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Consignment final exportation country</ccts:Name>
            <ccts:Description>[3331] To identify the country from which goods have been or will be consigned prior to final importation.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="38">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Consignment first destination country</ccts:Name>
            <ccts:Description>[3219] To identify the country where a consignment is off-loaded from the means of transport used for the original exportation.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="39">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of production</ccts:Name>
            <ccts:Description>Country where item has been or will be produced.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="40">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of trading</ccts:Name>
            <ccts:Description>Country where item has been or will be traded.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="41">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Consignment entry customs office location</ccts:Name>
            <ccts:Description>[3089] To identify the customs office at which the goods enter the customs territory of destination.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="42">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Consignment exit customs office location</ccts:Name>
            <ccts:Description>[3097] To identify the customs office at which the goods leave or are intended to leave the customs territory of despatch.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="43">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of Customs examination</ccts:Name>
            <ccts:Description>Place where Customs undertakes a physical inspection of goods to satisfy themselves that the goods' nature, origin, condition, quantity and value are in accordance with the particulars furnished on the goods declaration (CCC).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="44">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of authentication of document</ccts:Name>
            <ccts:Description>(3410) Place where document is signed or otherwise authenticated. Synonym: Place of issue of document.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="45">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Customs office of destination (transit)</ccts:Name>
            <ccts:Description>(3086) Customs office at which a transit operation is terminated. Synonym: Customs office of transit termination.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="46">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Region of despatch</ccts:Name>
            <ccts:Description>Region from which goods are despatched between countries of a Customs union.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="47">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Region of destination</ccts:Name>
            <ccts:Description>Region known to the consignor or his agent at the time of despatch to be the final region to which the goods are to be delivered.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="48">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Region of production</ccts:Name>
            <ccts:Description>Region where item has been or will be produced.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="49">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Transit country</ccts:Name>
            <ccts:Description>[3263] Country through which a goods or passengers are routed between the country of original departure and final destination.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="50">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Transit customs office location</ccts:Name>
            <ccts:Description>[3107] To identify the customs office which is responsible for transit formalities en-route.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="51">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of invalid transit guarantee</ccts:Name>
            <ccts:Description>Country in which the security or guarantee for the movement of goods under a transit procedure is not valid.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="52">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of destination (transit)</ccts:Name>
            <ccts:Description>Country at which a Customs transit operation is terminated. Synonym: Country of transit termination.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="53">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Charge and freight due from</ccts:Name>
            <ccts:Description>Place or point from which charges and freight are charged.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="54">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Manufacturing department</ccts:Name>
            <ccts:Description>A department within the manufacturing area (e.g. lacquering, assembly).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="55">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Freight charge payable to location</ccts:Name>
            <ccts:Description>[3102] Name of a place to which a transport charges tariff applies or where freight and other related charges are to be or have been incurred.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="56">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>End of chargeable section</ccts:Name>
            <ccts:Description>Last rail station in a predefined section of the chargeable voyage. A complete voyage may be divided in sections, even within one country, that are separately chargeable using different tariff rules (split tariffs).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="57">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of payment</ccts:Name>
            <ccts:Description>Name of the location at which freight and charges for main transport are payable.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="58">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Full track loading or unloading</ccts:Name>
            <ccts:Description>Identification of the station proceeding to the loading or unloading of a rail wagon on a full track site. (Used only when a rail station is obliged to transfer the load on another wagon for technical reasons - e.g. damage).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="59">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of loss</ccts:Name>
            <ccts:Description>To identify the location where the loss occurred.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="60">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of arrival</ccts:Name>
            <ccts:Description>Place at which the transport means arrives.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="61">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Next port of call</ccts:Name>
            <ccts:Description>(3095) Next port which the vessel is going to call upon.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="62">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>On-carriage port</ccts:Name>
            <ccts:Description>Port of discharge at which the cargo is discharged from the vessel, used for transport after the main transport (transit port).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="63">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Sub-project location</ccts:Name>
            <ccts:Description>A place at which works occur referring to a sub-project.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="64">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>First optional place of discharge</ccts:Name>
            <ccts:Description>The first optional place or port of discharge as mentioned on the transport document where cargo can be discharged at the option of the shipper.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="65">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Final port or place of discharge</ccts:Name>
            <ccts:Description>Name of the seaport, airport, freight terminal, rail station or other place at which the goods (cargo) are finally (to be) unloaded from the means of transport used for their carriage according to the transport contract.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="66">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Express railway station</ccts:Name>
            <ccts:Description>Railway station offering express transportation services.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="67">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mixed cargo railway station</ccts:Name>
            <ccts:Description>Railway station offering mixed cargo transportation services .</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="68">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Second optional place of discharge</ccts:Name>
            <ccts:Description>The second optional place or port of discharge as mentioned on the transport document where cargo can be discharged at the option of shipper.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="69">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Next non-discharge port of call</ccts:Name>
            <ccts:Description>A code to identify the next port of call for a vessel where no cargo will be discharged.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="70">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Third optional place of discharge</ccts:Name>
            <ccts:Description>The third optional place or port of discharge as mentioned on the transport document where cargo can be discharged at the option of the shipper.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="71">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Reconsolidation point</ccts:Name>
            <ccts:Description>A place where cargo is reconsolidated.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="72">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Fourth optional place of discharge</ccts:Name>
            <ccts:Description>The fourth optional place or port of discharge as mentioned on the transport document where cargo can be discharged at the option of the shipper.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="73">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Bill of lading release office</ccts:Name>
            <ccts:Description>A location where bills of lading are released to customers.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="74">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Transhipment excluding this place</ccts:Name>
            <ccts:Description>Place/location where a transhipment from a means of transport to another means of transport is not authorised.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="75">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Transhipment limited to this place</ccts:Name>
            <ccts:Description>Only place/location where a transhipment from a means of transport to another means of transport is authorised.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="76">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Original port of loading</ccts:Name>
            <ccts:Description>(3099) The port where the goods were first loaded on a vessel.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="77">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>First port of call - non-discharging</ccts:Name>
            <ccts:Description>Port in the country of destination where the conveyance initially arrives from the 'last place/port of call of conveyance' and where a conveyance will not be discharging cargoes.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="78">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>First port of call - discharging</ccts:Name>
            <ccts:Description>Port in the country of destination where the conveyance initially arrives from the 'last place/port of call of conveyance' and where a conveyance will be discharging cargoes.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="79">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place/port of first entry</ccts:Name>
            <ccts:Description>Place or port where final documentation is filed for Customs entry processing.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="80">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of despatch</ccts:Name>
            <ccts:Description>Place at which the goods are taken over for carriage (operational term), if different from the transport contract place of acceptance (see: 10). Synonym: Place of origin of carriage.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="81">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Fifth optional place of discharge</ccts:Name>
            <ccts:Description>The fifth optional place or port of discharge as mentioned on the transport document where cargo can be discharged at the option of the shipper.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="82">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Pre-carriage port</ccts:Name>
            <ccts:Description>Port of loading at which the cargo is loaded on the pre-carriage vessel used for the transport prior to the main transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="83">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of delivery (by on carriage)</ccts:Name>
            <ccts:Description>[3358] Place to which the goods are to be finally delivered.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="84">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Transport contract place of acceptance</ccts:Name>
            <ccts:Description>Place at which the goods are taken over by the carrier according to the contract of carriage.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="85">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Transport contract place of destination</ccts:Name>
            <ccts:Description>Place to which the goods are destined.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="86">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of valid transit guarantee</ccts:Name>
            <ccts:Description>Country in which the security or guarantee for the movement of goods under a transit procedure is valid.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="87">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place/port of conveyance initial arrival</ccts:Name>
            <ccts:Description>Place/port in the country of destination where the conveyance initially arrives from the "Last place/port of call of conveyance" (125).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="88">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of receipt</ccts:Name>
            <ccts:Description>Identification of the location at which the cargo is actually received.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="89">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of registration</ccts:Name>
            <ccts:Description>(3105) Place where the registration occurs.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="90">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Special treatment place</ccts:Name>
            <ccts:Description>Place where one or more special treatments have happened or must happen.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="91">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of document issue</ccts:Name>
            <ccts:Description>The place or location where a document is issued.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="92">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Routing</ccts:Name>
            <ccts:Description>Indication of a routing place.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="93">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Station of application of additional costs</ccts:Name>
            <ccts:Description>Rail station where, according to the transport contract, some chargeable operations must happen (re-weighting, re-fixing of the load, control on equipment and on consignment, etc.).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="94">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Previous port of call</ccts:Name>
            <ccts:Description>Previous port which the vessel has called upon.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="95">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Sailing destination area</ccts:Name>
            <ccts:Description>Geographical area to which the vessel's trip is destined.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="96">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of lodgement of documents</ccts:Name>
            <ccts:Description>Customs station where, required documents for Customs declarations, have been lodged.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="97">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Optional place of discharge</ccts:Name>
            <ccts:Description>The optional place or port of discharge as mentioned on the bill of lading where cargo is discharged at the option of the shipper.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="98">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of empty equipment despatch</ccts:Name>
            <ccts:Description>The location from which empty equipment is despatched.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="99">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of empty equipment return</ccts:Name>
            <ccts:Description>The location to which empty equipment is returned.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="100">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place/port of warehouse entry</ccts:Name>
            <ccts:Description>Location (e.g. district) within a Customs territory where a warehouse entry was filed to enter merchandise into a Customs bonded warehouse.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="101">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of first sale</ccts:Name>
            <ccts:Description>Name of country where firstly a sale took place.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="103">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of transfer</ccts:Name>
            <ccts:Description>Place at which goods are transferred from one carrier to another (contractual term).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="104">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of deconsolidation</ccts:Name>
            <ccts:Description>Place where a large consignment is de-grouped into smaller consignments.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="105">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of consumption</ccts:Name>
            <ccts:Description>Place/location where goods enter the marketplace (commerce) of the importing country.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="106">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Region of origin</ccts:Name>
            <ccts:Description>Region in which the goods have been produced or manufactured according to the criteria laid down for the purposes of the application of the Customs tariff, of quantitative restrictions or of any other measures related to trade (see: 3238).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="107">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of consolidation</ccts:Name>
            <ccts:Description>Place where smaller consignments of goods are grouped together into a large consignment to be transported as a larger unit.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="108">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Rate combination point</ccts:Name>
            <ccts:Description>[3369] Point over which sector rates are combined.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="109">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of prolongation decision of delivery delay</ccts:Name>
            <ccts:Description>Place where it has been decided to prolong the delivery delay.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="110">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Recharging place/location</ccts:Name>
            <ccts:Description>Place/location where a consignment has been changed of destination and is subject to a recharge note. (Complementary orders to modify the routing of the transport may be given, upon which a new charge calculation may be applied by the carrier).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="111">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Customs office of despatch</ccts:Name>
            <ccts:Description>Customs office from which goods are despatched between countries of a Customs union.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="112">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Region of equipment availability</ccts:Name>
            <ccts:Description>Region in which a piece of equipment is requested to be made available for on-hire.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="113">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of despatch</ccts:Name>
            <ccts:Description>Country from which goods are despatched within a Customs union.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="114">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Customs office of export</ccts:Name>
            <ccts:Description>Customs office from which goods are taken out of the Customs territory (CCC).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="115">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Free zone of export</ccts:Name>
            <ccts:Description>Foreign free zone (desc: see 1131 = 131) from which goods are exported to the country of importation.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="116">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Region of export/despatch</ccts:Name>
            <ccts:Description>Region from which the goods were initially exported to the importing country without any commercial transaction taking place in intermediate countries. Region of despatch: region from which goods are despatched between countries of a Customs union.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="117">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of collection</ccts:Name>
            <ccts:Description>Place where goods are to be or could be picked up (collected).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="118">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Customs office of departure</ccts:Name>
            <ccts:Description>Customs office at which a Customs transit operation commences (CCC).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="119">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Transit guarantee customs office location</ccts:Name>
            <ccts:Description>[3111] To identify the Customs office at which a security or guarantee for the movement of goods under a transit procedure is lodged.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="120">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of transhipment</ccts:Name>
            <ccts:Description>Country where goods are transferred under Customs control from the importing means of transport to the exporting means of transport within the area of one Customs office which is the office of both importation and exportation (CCC).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="122">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Customs office of destination</ccts:Name>
            <ccts:Description>Customs office where the goods are to be cleared (CCC).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="123">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Wagon-load railway station</ccts:Name>
            <ccts:Description>A railway station where rail wagons are loaded.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="124">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Siding</ccts:Name>
            <ccts:Description>A short railway track for loading or unloading rail wagons, or bypassing of trains, connected with a main track by switch.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="125">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Last place/port of call of conveyance</ccts:Name>
            <ccts:Description>Conveyance departed from this last foreign place/port of call to go to "Place/port of conveyance initial arrival" (87).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="126">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Country of previous Customs procedure</ccts:Name>
            <ccts:Description>Country in which the Customs declaration for the previous Customs procedure has been lodged.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="127">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Customs office of registration of previous Customs declaration</ccts:Name>
            <ccts:Description>Customs office where the previous Customs declaration has been lodged.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="128">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Participant sender location</ccts:Name>
            <ccts:Description>Place where a participant in the movement of goods is located and can be contacted.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="129">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Wage negotiation district</ccts:Name>
            <ccts:Description>The district to which workers belong for the purposes of union wage negotiation.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="130">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of ultimate destination of conveyance</ccts:Name>
            <ccts:Description>Seaport, airport, freight terminal, rail station or other place to which a means of transport is ultimately destined.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="131">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of loading of empty equipment</ccts:Name>
            <ccts:Description>Seaport, airport, freight terminal, rail station or other place where empty equipment (e.g. containers) was loaded onto means of transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="132">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of discharge of empty equipment</ccts:Name>
            <ccts:Description>Seaport, airport, freight terminal, rail station or other place where empty equipment (e.g. containers) was unloaded from means of transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="133">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Region of delivery</ccts:Name>
            <ccts:Description>(3246) Region to which the goods are to be finally delivered under transport contract terms (operational term).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="134">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Petroleum warehouse</ccts:Name>
            <ccts:Description>Bonded petroleum warehouse or the supplier source.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="135">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Place of entry (Customs)</ccts:Name>
            <ccts:Description>Place at which the goods enter the Customs territory.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="136">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Living animals care place</ccts:Name>
            <ccts:Description>Place where living animal cares are provided.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="137">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Re-icing place</ccts:Name>
