<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:clm65189AllowanceChargeID="urn:un:unece:uncefact:codelist:standard:UNECE:AllowanceChargeIdentificationCode:D17B" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" targetNamespace="urn:un:unece:uncefact:codelist:standard:UNECE:AllowanceChargeIdentificationCode:D17B" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.3">
<!-- ======================================================================= -->
<!-- ===== Element Declarations                                        ===== -->
<!-- ======================================================================= -->
<!-- ===== Root Element                                                ===== -->
<!-- ======================================================================= -->
  <xsd:element name="AllowanceChargeIdentificationCode" type="clm65189AllowanceChargeID:AllowanceChargeIdentificationCodeContentType"/>
<!-- ================================================================== -->
<!-- ===== Type Definitions                                       ===== -->
<!-- ================================================================== -->
<!-- ===== Type Definition: AllowanceChargeIdentificationCodeContentType ===== -->
<!-- ================================================================== -->
  <xsd:simpleType name="AllowanceChargeIdentificationCodeContentType">
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="1">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Handling commission</ccts:Name>
            <ccts:Description>Fee for the processing of documentary credit, collection and payment which are charged to the customer.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="2">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Amendment commission</ccts:Name>
            <ccts:Description>Fee for amendments in documentary credit and collection business (not extensions and increases of documentary credits).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="3">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Acceptance commission</ccts:Name>
            <ccts:Description>Fee for the acceptance of draft in documentary credit and collection business which are drawn on us (also to be seen as a kind of 'guarantee commission').</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="4">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Commission for obtaining acceptance</ccts:Name>
            <ccts:Description>Fee for obtaining an acceptance under collections on the basis of 'documents against acceptance'.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="5">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Commission on delivery</ccts:Name>
            <ccts:Description>Fee for delivery of documents without corresponding payment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="6">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Advising commission</ccts:Name>
            <ccts:Description>Fee for advising documentary credits (charged also in case of confirmed credits).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="7">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Confirmation commission</ccts:Name>
            <ccts:Description>Fee for confirmation of credit.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="8">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Deferred payment commission</ccts:Name>
            <ccts:Description>Fee for the deferred payment period under documentary credits confirmed by bank. This fee are charges for the period from presentation of the document until due date of payment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="9">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Commission for taking up documents</ccts:Name>
            <ccts:Description>Fee charged to the foreign bank for the processing of documentary credit.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="10">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Opening commission</ccts:Name>
            <ccts:Description>Fee for opening revocable documentary credit.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="11">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Fee for payment under reserve</ccts:Name>
            <ccts:Description>Fee charged to the customer for discrepancies in credit documents in the case of which the bank have to stipulate payment under reserve.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="12">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Discrepancy fee</ccts:Name>
            <ccts:Description>Fee charged to the foreign bank for discrepancies in credit documents.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="13">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Domicilation commission</ccts:Name>
            <ccts:Description>Fee for the domicilation of bills with the bank.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="14">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Commission for release of goods</ccts:Name>
            <ccts:Description>Commission for the release of goods sent to the bank.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="15">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Collection commission</ccts:Name>
            <ccts:Description>Fee for settling collections on the basis of 'documents against payments'.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="16">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Negotiation commission</ccts:Name>
            <ccts:Description>Fee for the purchase of documents under sight credit for the first ten days.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="17">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Return commission</ccts:Name>
            <ccts:Description>Fee for cheques, bills and collections returned unpaid and/or recalled.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="18">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>B/L splitting charges</ccts:Name>
            <ccts:Description>Fee for the splitting of bills of lading.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="19">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Trust commission</ccts:Name>
            <ccts:Description>Fee for the handling on a fiduciary basis of imported goods that have been warehoused.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="20">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Transfer commission</ccts:Name>
            <ccts:Description>Fee for the transfer of transferable documentary credits.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="21">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Commission for opening irrevocable documentary credits</ccts:Name>
            <ccts:Description>Fee for opening irrevocable documentary credits. This fee is a kind of 'Guarantee commission' as compensation for the commitment into which the bank have entered on the customers behalf; similar to confirmation commission, acceptance commission.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="22">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Pre-advice commission</ccts:Name>
            <ccts:Description>Fee for the pre-advice of a documentary credit.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="23">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Supervisory commission</ccts:Name>
            <ccts:Description>Fee for the supervising unconfirmed documentary credits with a deferred payment period.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="24">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Model charges</ccts:Name>
            <ccts:Description>Fee for decoding telex messages.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="25">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Risk commission</ccts:Name>
            <ccts:Description>Commission in addition to the confirmation commission for documentary credits from sensitive countries.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="26">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Guarantee commission</ccts:Name>
            <ccts:Description>Commission for drawing up guaranties.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="27">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Reimbursement commission</ccts:Name>
            <ccts:Description>Fee for reimbursement of, for example, documentary credits.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="28">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Stamp duty</ccts:Name>
            <ccts:Description>Tax payable on bills in accordance with national bill of exchange legislation.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="29">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Brokerage</ccts:Name>
            <ccts:Description>Brokers commission arising, in trade with foreign currencies.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="30">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Bank charges</ccts:Name>
            <ccts:Description>Charges deducted/claimed by other banks involved in the transaction.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="31">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Bank charges information</ccts:Name>
            <ccts:Description>Charges not included in the total charge amount i.e. the charges are for information only.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="32">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Courier fee</ccts:Name>
            <ccts:Description>Fee for use of courier service.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="33">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Phone fee</ccts:Name>
            <ccts:Description>Fee for use of phone.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="34">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Postage fee</ccts:Name>
            <ccts:Description>Fee for postage.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="35">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>S.W.I.F.T. fee</ccts:Name>
            <ccts:Description>Fee for use of S.W.I.F.T.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="36">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Telex fee</ccts:Name>
            <ccts:Description>Fee for telex.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="37">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Penalty for late delivery of documents</ccts:Name>
            <ccts:Description>Penalty imposed when documents are delivered late.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="38">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Penalty for late delivery of valuation of works</ccts:Name>
            <ccts:Description>Penalty imposed when valuation of works is delivered late.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="39">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Penalty for execution of works behind schedule</ccts:Name>
            <ccts:Description>Penalty imposed when the execution of works is behind schedule.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="40">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Other penalties</ccts:Name>
            <ccts:Description>Penalty imposed for other reasons.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="41">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Bonus for works ahead of schedule</ccts:Name>
            <ccts:Description>Bonus for completing work ahead of schedule.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="42">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Other bonus</ccts:Name>
            <ccts:Description>Bonus earned for other reasons.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="44">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Project management cost</ccts:Name>
            <ccts:Description>Cost for project management.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="45">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Pro rata retention</ccts:Name>
            <ccts:Description>Proportional retention charge.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="46">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Contractual retention</ccts:Name>
            <ccts:Description>Contractual retention charge.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="47">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Other retentions</ccts:Name>
            <ccts:Description>Retention charge not otherwise specified.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="48">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Interest on arrears</ccts:Name>
            <ccts:Description>Interest for late payment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="49">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Interest</ccts:Name>
            <ccts:Description>Cost of using money.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="50">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Charge per credit cover</ccts:Name>
            <ccts:Description>Unit charge per credit cover established.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="51">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Charge per unused credit cover</ccts:Name>
            <ccts:Description>Unit charge per unused credit cover.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="52">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Minimum commission</ccts:Name>
            <ccts:Description>Minimum commission charge.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="53">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Factoring commission</ccts:Name>
            <ccts:Description>Commission charged for factoring services.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="54">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Chamber of commerce charge</ccts:Name>
            <ccts:Description>Identifies the charges from the chamber of commerce.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="55">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Transfer charges</ccts:Name>
            <ccts:Description>Charges for transfer.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="56">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Repatriation charges</ccts:Name>
            <ccts:Description>Charges for repatriation.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="57">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Miscellaneous charges</ccts:Name>
            <ccts:Description>Not specifically defined charges.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="58">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Foreign exchange charges</ccts:Name>
            <ccts:Description>Charges for foreign exchange.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="59">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Agreed debit interest charge</ccts:Name>
            <ccts:Description>Charge for agreed debit interest</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="60">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Manufacturer's consumer discount</ccts:Name>
            <ccts:Description>A discount given by the manufacturer which should be passed on to the consumer.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="61">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Fax advice charge</ccts:Name>
            <ccts:Description>Charge for fax advice.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="62">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Due to military status</ccts:Name>
            <ccts:Description>Allowance granted because of the military status.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="63">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Due to work accident</ccts:Name>
            <ccts:Description>Allowance granted to a victim of a work accident.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="64">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Special agreement</ccts:Name>
            <ccts:Description>An allowance or charge as specified in a special agreement.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="65">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Production error discount</ccts:Name>
            <ccts:Description>A discount given for the purchase of a product with a production error.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="66">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>New outlet discount</ccts:Name>
            <ccts:Description>A discount given at the occasion of the opening of a new outlet.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="67">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Sample discount</ccts:Name>
            <ccts:Description>A discount given for the purchase of a sample of a product.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="68">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>End-of-range discount</ccts:Name>
            <ccts:Description>A discount given for the purchase of an end-of-range product.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="69">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Charge for a customer specific finish</ccts:Name>
            <ccts:Description>A charge for the addition of a customer specific finish to a product.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="70">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Incoterm discount</ccts:Name>
            <ccts:Description>A discount given for a specified Incoterm.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="71">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Point of sales threshold allowance</ccts:Name>
            <ccts:Description>Allowance for reaching or exceeding an agreed sales threshold at the point of sales.</ccts:Description>
          </xsd:documentation>
